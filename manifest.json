{
  "name": "Woodworking automated fence",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "description": "Computer managed fence for table saw or router table",
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "fileSystem",
    "power",
    "serial",
    "storage"
  ]
}
